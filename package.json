{
  "name": "api-m5ls5e-com",
  "version": "0.1.0",
  "description": "Serverless functions I use as API endpoints in various projects",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "scripts": {
    "start": "vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/api-m5ls5e-com"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "",
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "cross-env": "7.0.3",
    "dayjs": "1.11.4",
    "dotenv": "16.0.1",
    "got": "12.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/eslint-parser": "7.18.9",
    "cross-env": "7.0.3",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1",
    "vercel": "27.3.3"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "commonjs": false,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 11,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "ignorePatterns": [
      "**/.history/*",
      "**/.vercel/*",
      "**/node_modules/*"
    ],
    "rules": {}
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2
  }
}
